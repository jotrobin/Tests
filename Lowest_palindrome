#include <iostream>
#include <math.h>


long low(long a)
{
    
    
    long d=0;
    long b[10];
     for(int i=1;i<10;i++)
         b[i]=0;
    long e=a;
    int n=0;
    while(e)
    {
        e=e/10;
        n++;
    }
    
    while(a)
    {
        long c;
        c=a%10;
        b[c]=c;
        a=a/10;
        
    }
    
    
    for(int i=1;i<10;i++)
    {
        if(b[i]!=0 && n>0)
        {
            d=d+pow(10,n-1)*b[i];
            n--;
          //  std::cout<<d<<"kk\n";
        }
        //std::cout<<b[i]<<"kk\n";
    }
    return d;
}

int main(int argc, const char * argv[]) {
    long d;
    std::cin>>d;
    std::cout<<low(d);
    return 0;
}
